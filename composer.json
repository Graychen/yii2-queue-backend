{
    "name": "graychen/yii2-queue-backend",
    "description": "Background display queue statistics",
    "type": "yii2-extension",
    "require": {
        "yiisoft/yii2": "^2.0",
        "yiisoft/yii2-queue": "^2.0",
        "yiisoft/yii2-redis": "^2.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "陈家辉",
            "email": "chenjiahui@haolianluo.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "graychen\\yii2\\queue\\backend\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "graychen\\yii2\\queue\\backend\\tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
